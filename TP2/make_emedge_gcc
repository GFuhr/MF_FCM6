include libconfig.in
MAKE = make  --no-print-directory
CC = gcc
CLINKER = $(CC)
SHELL = /bin/sh
MV = mv
SRC = src
PATHFILE = .
OBJ = $(PATHFILE)/obj/$(CC)
EXE = $(PATHFILE)/bin
ADD_LIB = $(ADD_LIB_CFG) -L$(LIB_PERS)
ADD_INC = $(ADD_INC_CFG) -I$(INC_PERS) 
HEADERS = $(SRC)/include
LIB_PERS = $(SRC)/libs/
HEADERS = $(SRC)/include
CFLAGS= -O0  -g -ggdb -fstrict-aliasing -ffast-math -std=c99 -ansi -pedantic -Wchar-subscripts -Wcomment -Wformat=2 -Wimplicit-int -Werror-implicit-function-declaration -Wmain -Wparentheses -Wsequence-point -Wreturn-type -Wswitch -Wtrigraphs -Wunused -Wuninitialized -Wunknown-pragmas -Wfloat-equal -Wundef -Wshadow -Wpointer-arith -Wbad-function-cast -Wwrite-strings -Wconversion -Wsign-compare -Waggregate-return -Wstrict-prototypes -Wmissing-prototypes -Wmissing-declarations -Wmissing-noreturn -Wformat -Wmissing-format-attribute -Wno-deprecated-declarations -Wpacked -Wredundant-decls -Wnested-externs -Winline -Wlong-long -Wunreachable-code -fopenmp -DH5Gcreate_vers=2 -DH5Dcreate_vers=2 -DH5Gopen_vers=2 -DH5Dopen_vers=2
LDFLAGS=$(ADD_LIB) -lm 
PRGM_O = 

all	: emedge_gcc.exe
clean:
	-rm $(OBJ)/*.o $(EXE)/emedge_gcc.exe
emedge_gcc.exe : $(PRGM_O)
	$(CC) $(CFLAGS) $(PRGM_O) $(CLIBS) $(LDFLAGS) -o $(EXE)/emedge_gcc.exe

